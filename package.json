{
  "name": "webmchan",
  "version": "0.0.1",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "package-linux": "ng build --configuration production && electron-packager . webmchan --overwrite --platform=linux --arch=x64 --icon=src/assets/icons/png/1024x1024.png --no-prune --ignore=/node_modules --ignore=/e2e --ignore=/src --out=release-builds/",
    "package-win": "ng build --configuration production && electron-packager . webmchan --overwrite --platform=win32 --arch=x64 --icon=src/assets/icons/win/favicon.ico --no-prune --ignore=/node_modules --ignore=/e2e --ignore=/src --out=release-builds/ --version-string.CompanyName=ASTRA1 --version-string.FileDescription=WHISHMASTER --version-string.ProductName=\"WEBM_WISHMASTER\"",
    "package-mac": "ng build --configuration production && electron-packager . webmchan --overwrite --platform=darwin --arch=x64 --icon=src/assets/icons/mac/icon.icns --no-prune --ignore=/node_modules --ignore=/e2e --ignore=/src --out=release-builds/",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.3.6",
    "@angular/cdk": "^13.3.6",
    "@angular/common": "^13.3.6",
    "@angular/compiler": "^13.3.6",
    "@angular/core": "^13.3.6",
    "@angular/flex-layout": "^13.0.0-beta.38",
    "@angular/forms": "^13.3.6",
    "@angular/material": "^13.3.6",
    "@angular/platform-browser": "^13.3.6",
    "@angular/platform-browser-dynamic": "^13.3.6",
    "@angular/router": "^13.3.6",
    "@fortawesome/angular-fontawesome": "^0.10.2",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@ngxs/devtools-plugin": "^3.7.3",
    "@ngxs/logger-plugin": "^3.7.3",
    "@ngxs/router-plugin": "^3.7.3",
    "@ngxs/store": "^3.7.3",
    "@sentry/browser": "^6.19.7",
    "electron": "^18.3.7",
    "plyr": "^3.7.2",
    "rxjs": "^7.5.5",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.5",
    "@angular/cli": "^13.3.5",
    "@angular/compiler-cli": "~13.3.6",
    "@angular/language-service": "~13.3.6",
    "@types/jasmine": "~4.0.3",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "~14.14.0",
    "codelyzer": "^5.2.2",
    "electron-packager": "^15.5.1",
    "jasmine": "~4.1.0",
    "jasmine-core": "~4.1.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.19",
    "karma-chrome-launcher": "~3.1.1",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "protractor": "~7.0.0",
    "ts-node": "^10.7.0",
    "tslint": "~6.1.3",
    "typescript": "~4.6.4"
  }

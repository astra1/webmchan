{
  "name": "webmchan",
  "version": "0.0.1",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "package-linux": "ng build --prod && electron-packager . webmchan --overwrite --platform=linux --arch=x64 --icon=src/assets/icons/png/1024x1024.png --no-prune --ignore=/node_modules --ignore=/e2e --ignore=/src --out=release-builds/",
    "package-win": "ng build --prod && electron-packager . webmchan --overwrite --platform=win32 --arch=x64 --icon=src/assets/icons/win/favicon.ico --no-prune --ignore=/node_modules --ignore=/e2e --ignore=/src --out=release-builds/ --version-string.CompanyName=ASTRA1 --version-string.FileDescription=WHISHMASTER --version-string.ProductName=\"WEBM_WISHMASTER\"",
    "package-mac": "ng build --prod && electron-packager . webmchan --overwrite --platform=darwin --arch=x64 --icon=src/assets/icons/mac/icon.icns --no-prune --ignore=/node_modules --ignore=/e2e --ignore=/src --out=release-builds/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.3",
    "@angular/cdk": "^7.0.2",
    "@angular/common": "~8.2.3",
    "@angular/compiler": "~8.2.3",
    "@angular/core": "~8.2.3",
    "@angular/flex-layout": "^7.0.0-beta.19",
    "@angular/forms": "~8.2.3",
    "@angular/http": "~7.0.1",
    "@angular/material": "^7.0.2",
    "@angular/platform-browser": "~8.2.3",
    "@angular/platform-browser-dynamic": "~8.2.3",
    "@angular/router": "~8.2.3",
    "@fortawesome/angular-fontawesome": "0.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.7",
    "@fortawesome/free-solid-svg-icons": "^5.4.2",
    "@ngx-pwa/local-storage": "^7.0.0",
    "angular2-hotkeys": "^2.1.3",
    "core-js": "^2.5.7",
    "electron": "^3.0.6",
    "hammerjs": "^2.0.8",
    "moment": "^2.22.2",
    "ngx-moment": "^3.1.0",
    "rxjs": "^6.3.3",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.0",
    "@angular/cli": "^8.3.0",
    "@angular/compiler-cli": "~8.2.3",
    "@angular/language-service": "~8.2.3",
    "@types/jasmine": "~2.8.9",

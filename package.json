{
  "name": "webmchan",
  "version": "0.0.1",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "package-linux": "ng build --configuration production && electron-packager . webmchan --overwrite --platform=linux --arch=x64 --icon=src/assets/icons/png/1024x1024.png --no-prune --ignore=/node_modules --ignore=/e2e --ignore=/src --out=release-builds/",
    "package-win": "ng build --configuration production && electron-packager . webmchan --overwrite --platform=win32 --arch=x64 --icon=src/assets/icons/win/favicon.ico --no-prune --ignore=/node_modules --ignore=/e2e --ignore=/src --out=release-builds/ --version-string.CompanyName=ASTRA1 --version-string.FileDescription=WHISHMASTER --version-string.ProductName=\"WEBM_WISHMASTER\"",
    "package-mac": "ng build --configuration production && electron-packager . webmchan --overwrite --platform=darwin --arch=x64 --icon=src/assets/icons/mac/icon.icns --no-prune --ignore=/node_modules --ignore=/e2e --ignore=/src --out=release-builds/",
    "postinstall": ""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.16",
    "@angular/cdk": "^12.2.13",
    "@angular/common": "^12.2.16",
    "@angular/compiler": "^12.2.16",
    "@angular/core": "^12.2.16",
    "@angular/flex-layout": "^12.0.0-beta.35",
    "@angular/forms": "^12.2.16",
    "@angular/material": "^12.2.13",
    "@angular/platform-browser": "^12.2.16",
    "@angular/platform-browser-dynamic": "^12.2.16",
    "@angular/router": "^12.2.16",
    "@fortawesome/angular-fontawesome": "^0.9.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@ngxs/devtools-plugin": "^3.7.0",
    "@ngxs/logger-plugin": "^3.7.0",
    "@ngxs/router-plugin": "^3.7.0",
    "@ngxs/store": "^3.7.0",
    "@sentry/browser": "^5.21.1",
    "@types/video.js": "^7.3.10",
    "electron": "^9.2.0",
    "plyr": "^3.6.2",
    "rxjs": "^6.6.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.17",
    "@angular/cli": "^12.2.17",
